function isSubsequence(s, t) {
    let i=0;
    let j=0;
    while(i<s.length && j<t.length){
        if(s[i]===t[j]){
            i++;
            j++;
        } else {
            j++;
        }
    }
    return i===s.length ? true : false
}

console.log(isSubsequence("abc", "ahbagdbc"));
console.log(isSubsequence("axc", "ahbgdc"));
